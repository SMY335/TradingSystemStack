# TradingSystemStack - Environment Variables Example
# Copy this file to .env and fill in your values

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=true

# Data Sources
YFINANCE_ENABLED=true
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
FRED_API_KEY=your_fred_api_key_here

# Database (Optional)
# MONGODB_URI=mongodb://localhost:27017
# MONGODB_DATABASE=trading_system

# Cache Settings
CACHE_ENABLED=true
CACHE_DIR=./cache
CACHE_TTL=3600

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/trading_system.log

# Development
DEBUG=false
TESTING=false

# CORS Settings
CORS_ORIGINS=*
CORS_CREDENTIALS=true
CORS_METHODS=*
CORS_HEADERS=*
